import deepFreeze from 'deep-freeze';

import { {{camelCase actionName}} } from './{{camelCase actionName}}.reducer';

describe('{{camelCase actionName}}Reducer', () => {
  it('returns initial state', () => {
    const action = {
      type: '@@INIT',
    };

    deepFreeze(action);

    const nextState = {{camelCase actionName}}Reducer(undefined, action);

    expect(nextState).toEqual(INITIAL_STATE);
  });

  it('returns existing state for unknown action', () => {
    const action = {
      type: '@@UNKNOWN',
    };

    const prevState = INITIAL_STATE;

    deepFreeze(action);
    deepFreeze(prevState);

    const nextState = {{camelCase actionName}}Reducer(prevState, action);

    expect(nextState).toBe(prevState);
  });
});
